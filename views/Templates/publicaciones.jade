//TEMPLATE PARA REALIZAR EL ALTA DE PUBLICACIONES

extends ../MasterTemplates/layout

block extraHeader
	include ../includes/links
block scripts
	include ../includes/scripts
block nav
	include ../includes/Leftnavigation

block content
	div(id='page-wrapper')
		div(class='row')
			div(class='col-lg-12')
				h1(class='page-header h1Extra') Nueva Publicacion
		div(id='load', class='row')
			div(id='listContent', class='contentList')
				form(role='form')
					label Cargar Imagenes
					div(class='form-inline')
						img(id='img1', alt='no cargo', src='images/camera.png')
					div(class='form-group')
						input(type='file', name='upload', class='upload', title='file', multiple)
